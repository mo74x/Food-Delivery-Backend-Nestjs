# Application
NODE_ENV=development
PORT=3000

# Database Configuration
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5433
DB_USERNAME=admin
DB_PASSWORD=complex_password
DB_DATABASE=food_delivery
